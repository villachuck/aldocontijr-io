{
  "store.home": {
    "blocks": [
      "flex-layout.row#banners-principales",
      "slider-layout#help-categories",      
      "rich-text#shelf-title",
      "flex-layout.row#shelf-recomendados",
      "flex-layout.row#banner-middle",
