    }
  },
  "flex-layout.row#shelf-recomendados": {
    "children": ["list-context.product-list#recomendados"]
  },
  "list-context.product-list#recomendados": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#recomendados-products"],
    "props": {
      "orderBy": "OrderByTopSaleDESC"
    }
  },
  "slider-layout#recomendados-products": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 2
      },
