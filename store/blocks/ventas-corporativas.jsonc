{
    "store.custom#ventas-corporativas": {
        "blocks": [
            "flex-layout.row#ventas-corporativas"
        ]
    },
    "flex-layout.row#ventas-corporativas": {
        "children": [
            "flex-layout.col#ventas-corporativas"
        ],
        "props": {
            "blockClass": "faqs-body"
        }
    },
    "flex-layout.col#ventas-corporativas": {
        "children":[
            "flex-layout.row#ventas-corporativas-titulo",
            "flex-layout.row#ventas-corporativas-body"
