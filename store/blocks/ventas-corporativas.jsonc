        "props":{
            "blockClass": "faqs-blocks"
        }
    },
    "flex-layout.row#ventas-corporativas-titulo": {
        "children":[
            "rich-text#ventas-corporativas-titulo"
        ]
    },
    "rich-text#ventas-corporativas-titulo": {
        "props": {
            "text": "#### Ventas Corporativas",
            "blockClass": "faqs-titulo"
        }
    },
    "flex-layout.row#ventas-corporativas-body": {
        "children":[
            "flex-layout.col#ventas-corporativas-body",
